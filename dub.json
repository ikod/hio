{
    "name": "hio",
    "authors": [
        "Igor Khasilev"
    ],
    "description": "async io, task and other",
    "license": "proprietary",
    "targetType": "library",
    "preBuildCommands": [
	    "cd $HIO_PACKAGE_DIR/submodules/http-parser && make package"
    ],
    "dependencies": {
        "nbuff": "~>0",
        "timingwheels": "*"
    },
    "buildTypes": {
        "release": {
            "buildOptions": ["releaseMode", "optimize", "inline"],
            "debugVersions": []
        }
    },
    "configurations": [
        {
            "name": "library"
        },
        {
            "name": "unittest",
            "debugVersions": ["nbuff", "hioselect", "hioepoll", "hiohttp", "timingwheels"]
        }
    ],
    "lflags": ["-L$HIO_PACKAGE_DIR/submodules/http-parser"],
    "libs-posix": ["cares", "http_parser"]

}
